<Application x:Class="BetcityAnnunciator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:BetcityAnnunciator.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converters:TitleConverter x:Key="TitleConverter"/>
        <converters:EnvironmentConverter x:Key="EnvironmentConverter" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    </Application.Resources>
</Application>
